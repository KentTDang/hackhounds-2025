{
  "expo": {
    "name": "hackhounds-app",
    "slug": "hackhounds-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "hackhounds-app",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kent6900.hackhoundsapp",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.kent6900.hackhoundsapp"
    },
    "web": {
      "bundler": "metro",
      "output": "server",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://hackhounds-app.expo.app"
        }
      ],
      [
        "@stripe/stripe-react-native",
        {
          "merchantIdentifier": "merchant.com.hackhoundsapp",
          "publishableKey": "pk_test_51R19DeDU3XZnI4mb6CIHBgNiqmNme72M7S00EvNEXpQ1hT2PJYUgVHdMUikRNzCFiqLYl7hrWhaG86AYoc76tzZA00mGaDg5QL"
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": "https://hackhounds-app.expo.app"
      },
      "eas": {
        "projectId": "9178498e-5d30-4d19-9ad2-76a720c9096a"
      }
    }
  }
}
